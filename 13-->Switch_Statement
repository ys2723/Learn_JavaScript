// Switch Statements

let grade = "F";

switch(grade){
  case("A"):
    console.log("You ACED!");
    break;
  case("B"):
    console.log("You did good");
    break;
  case("C"):
    console.log("You didn't do good");
    break;
  case("D");
    console.log("You did bad!");
    break;
  case("F"):
    console.log("You Failed");
    break;
  default:
    console.log("N");
